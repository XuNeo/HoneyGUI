# building script for HoneyGUI

from building import *

cwd = GetCurrentDir()
CPPPATH = [cwd]
src = Split("""
""")

if GetDepend('CONFIG_REALTEK_BUILD_GUI_BASE_DEMO'):
    src += ['rtk_gui_base_demo.c']

if GetDepend('CONFIG_REALTEK_BUILD_GUI_PAINTENGINE_DEMO'):
    src += ['rtk_gui_paintengine_demo.c']



group = DefineGroup('realgui/demo', src, depend = [''], CPPPATH = CPPPATH)

Return('group')
